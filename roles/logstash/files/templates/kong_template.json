curl -XPUT "http://localhost:9200/_template/apilogs_template" -H "Content-Type: application/json" -d'
{
  "version" : 1001,
  "index_patterns": ["apilogs*"],
  "settings" : {
    "index.refresh_interval" : "5s",
    "number_of_shards" : 1,
    "number_of_replicas": 0
  },
  "mappings" : {
    "kong": {
      "date_detection": false,
      "properties": {
        "started_at": {
          "type":"date"
        },
        "@timestamp": {
          "type":"date"
        }
      }
    }
  }
}'
